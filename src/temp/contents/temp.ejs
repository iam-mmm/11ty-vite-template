---
pagination:
  data: globalDataSet.myData
  size: 1
permalink: "<%- pagination.items[0].name -%>.html"
---
<!doctype html>
<html lang="ja">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>タイトル</title>
    </head>
    <body>
        <%- include('_includes/header') %>
        <%- include(pagination.items[0].content) %>
        <ol>
            <% for (var item of pagination.items) { %>
                <li><%= item.name %></p>
            <% } %>
        </ol>
    </body>
</html>